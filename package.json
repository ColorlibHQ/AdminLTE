    "jquery": "^3.6.0",
    "jquery-knob-chif": "^1.2.13",
    "jquery-mapael": "^2.2.0",
    "jquery-mousewheel": "^3.1.13",
    "jquery-ui-dist": "^1.13.1",
    "jquery-validation": "^1.19.5",
