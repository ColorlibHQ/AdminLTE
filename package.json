    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "astro": "^4.0.6",
    "autoprefixer": "^10.4.16",
    "bootstrap": "^5.3.2",
    "bundlewatch": "^0.3.3",
    "clean-css-cli": "^5.6.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-xo": "^0.43.1",
