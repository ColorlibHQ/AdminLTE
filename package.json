    "daterangepicker": "^3.1.0",
    "dropzone": "^5.9.3",
    "ekko-lightbox": "^5.3.0",
    "fastclick": "^1.0.6",
    "filterizr": "^2.2.4",
    "flag-icons": "^6.6.3",
    "flot": "^4.2.2",
    "fs-extra": "^10.1.0",
    "fullcalendar": "^5.11.0",
    "icheck-bootstrap": "^3.0.1",
    "inputmask": "^5.0.7",
